{
  "name": "yarn-plugins",
  "packageManager": "yarn@3.7.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --list-different \"packages/**/{sources,test}/**/*.{ts,json,md}\"",
    "format:fix": "prettier --write \"packages/**/{sources,test}/**/*.{ts,json,md}\"",
    "lint": "eslint \"packages/**/{sources,test}/**/*.ts\"",
    "lint:fix": "eslint \"packages/**/{sources,test}/**/*.ts\" --fix",
    "update-sdk": "yarn dlx @yarnpkg/sdks vscode"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@yarnpkg/builder": "^3.3.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "author": {
    "email": "lembdev@gmail.com",
    "name": "Alexander Vyzhanov"
  }
}
