{
  "name": "yarn-plugins",
  "packageManager": "yarn@3.2.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --list-different \"packages/**/{sources,test}/**/*.{ts,json,md}\"",
    "format:fix": "prettier --write \"packages/**/{sources,test}/**/*.{ts,json,md}\"",
    "lint": "eslint \"packages/**/{sources,test}/**/*.ts\"",
    "lint:fix": "eslint \"packages/**/{sources,test}/**/*.ts\" --fix",
    "update-sdk": "yarn dlx @yarnpkg/sdks vscode"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@yarnpkg/builder": "^3.2.2",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.1",
    "prettier": "^2.6.2",
    "typescript": "^4.7.3"
  },
  "author": {
    "email": "lembdev@gmail.com",
    "name": "Alexander Vyzhanov"
  }
}
