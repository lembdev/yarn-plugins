{
  "name": "yarn-plugins",
  "packageManager": "yarn@3.4.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --list-different \"packages/**/{sources,test}/**/*.{ts,json,md}\"",
    "format:fix": "prettier --write \"packages/**/{sources,test}/**/*.{ts,json,md}\"",
    "lint": "eslint \"packages/**/{sources,test}/**/*.ts\"",
    "lint:fix": "eslint \"packages/**/{sources,test}/**/*.ts\" --fix",
    "update-sdk": "yarn dlx @yarnpkg/sdks vscode"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@yarnpkg/builder": "^3.2.7",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "author": {
    "email": "lembdev@gmail.com",
    "name": "Alexander Vyzhanov"
  }
}
